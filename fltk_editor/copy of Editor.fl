Function {cb_TabNext()} {open return_type void
} {
  code {//This should do the next function for a tab 
  int			num_kids;
  Fl_Widget	* const *kids;

  if ((num_kids = txt_Tab_Edit->children()) == 0)
    return;

  for (kids = txt_Tab_Edit->array(); num_kids > 0; kids ++, num_kids --)
  {
    if ((*kids)->visible())
      break;
  }

  if (num_kids > 1)
    txt_Tab_Edit->value(kids[1]);} {}
} 

Function {cb_TabPrev()} {open return_type void
} {
  code {//This should do the previous function for a tab 
 int			num_kids;
  Fl_Widget	* const *kids;


  if ((num_kids = txt_Tab_Edit->children()) == 0)
    return;

  for (kids = txt_Tab_Edit->array(); num_kids > 0; kids ++, num_kids --)
    if ((*kids)->visible())
      break;

  if (num_kids > 0 && num_kids < txt_Tab_Edit->children())
    txt_Tab_Edit->value(kids[-1]);} {}
} 
