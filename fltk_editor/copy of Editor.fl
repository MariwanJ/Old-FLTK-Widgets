} {
  code {// Open a file 
  char filename[256] = "";
  char *newfile = fl_file_chooser("HTML Files (*.html)\\tImage Files (*.{bmp,gif,jpg,png})", "*.htm*",  filename);
  if (newfile != NULL){
   strcpy(filename,newfile); 
     load_file(filename);
}} {}
