Function {cb_ShowPicturePreview(void)} {open return_type void
} {
  code {// Here we should write our code for previewing a picture file..

 if (!(previewBox->visible())) // if the preview Box is not visible .. do nothing.
       return;
   char preview_text_ [1024];
   char	      filename[4096];	// Current filename
   Fl_Shared_Image	*image,		// New image
	        		*oldimage;	// Old image
   int			pbw, pbh;	// Width and height of preview box
   int			w, h;		// Width and height of preview image

     char* tempfilename=(char*)(fl_File_View->text(fl_File_View->value()));
	 snprintf(filename,sizeof(filename),"%s%s",(direcotryBrowser->GetItemToDirectory(direcotryBrowser->Selection())), tempfilename);
	 cout <<filename<<endl;
    ///fl_File_View->value() ama naui file akaya

 // if (!preview_box->value()) return;
  fl_register_images();
  if ((filename == NULL)) 
    image = NULL;
  else 
  {
    window->cursor(FL_CURSOR_WAIT);
    Fl::check();

    image = Fl_Shared_Image::get(filename);

    if (image) {
      window->cursor(FL_CURSOR_DEFAULT);
      Fl::check();
    }
  }

  oldimage = (Fl_Shared_Image *)previewBox->image();

  if (oldimage) oldimage->release();

  previewBox->image(0);

  if (!image)  // text...
  {
    FILE	*fp;
    int		bytes;
    char	*ptr;
    int event_type=Fl::event_clicks();
     if (event_type>0)
     {
       load_file(filename);
     }
     else
       if (filename) 
         fp = fopen(filename, "rb");
       else 
         fp = NULL;

    if (fp != NULL)
     {
      // Try reading the first 1k of data for a label...
      bytes = fread(preview_text_, 1, sizeof(preview_text_) - 1, fp);
      preview_text_[bytes] = '\\0';
      fclose(fp);
    } else {
      // Assume we can't read any data...
      preview_text_[0] = '\\0';
    }

    window->cursor(FL_CURSOR_DEFAULT);
    Fl::check();

    // Scan the buffer for printable chars...
    for (ptr = preview_text_; *ptr && (isprint(*ptr & 255) || isspace(*ptr & 255)); ptr ++);
        if (*ptr || ptr == preview_text_)
        {
              // Non-printable file, just show a big ?...
               previewBox->label(filename ? "?" : 0);
               previewBox->align(FL_ALIGN_CLIP);
               previewBox->labelsize(100);
               previewBox->labelfont(FL_HELVETICA);
         }
          else 
        {
           // Show the first 1k of text...
            int size = previewBox->h() / 20;
            if (size < 6) 
               size = 6;
            else 
             if (size > 14) 
               size = 14;

            previewBox->label(preview_text_);
            previewBox->align((Fl_Align)(FL_ALIGN_CLIP | FL_ALIGN_INSIDE | FL_ALIGN_LEFT | FL_ALIGN_TOP));
            previewBox->labelsize((uchar)size);
            previewBox->labelfont(FL_COURIER);
        }
  } 
  else
 { // show the image ... 
   /// but this should not be done if the box is not shown!!
         if (((Fl_Tabs*)(tab_FileManager->value())!=(tab_Picture_View)))
               return; // do nothing
    pbw = previewBox->w() - 20;
    pbh = previewBox->h() - 20;
// check this to get exact size to the preview box or to maximize it 
    if (image->w() > pbw || image->h() > pbh)
    {
          w   = pbw;
          h   = w * image->h() / image->w();

         if (h > pbh) 
         {
	      h = pbh;
	      w = h * image->w() / image->h();
          }

         oldimage = (Fl_Shared_Image *)image->copy(w, h);
         previewBox->image((Fl_Image *)oldimage);
         image->release();
    } 
    else 
    {
      previewBox->image((Fl_Image *)image);
      
    }
    // this is to make the alignment of the picture as top-left 
    previewBox->align(FL_ALIGN_CLIP | FL_ALIGN_INSIDE | FL_ALIGN_LEFT | FL_ALIGN_TOP);
    previewBox->label(0);
  }
  previewBox->redraw();} {}
} 

Function {GetPixelColorOfImage(void *imagePointer, int x,int y,  uchar &r, uchar  &g, uchar &b)} {open return_type void
} {
  code {if (!(imagePointer))
    {
       return;                 
    }
    
 Fl_Image *ImageToBeRead=(Fl_Image *)(imagePointer);
 if (((ImageToBeRead->h()+previewBox->y()<y)|| (ImageToBeRead->w()+previewBox->x()<x)))
    {
       // The clicked X,Y is outside the picture 
       return;                                              
                                                
    }

   const char *buf = ImageToBeRead->data()[0];
   long index = (y * ImageToBeRead->w() * ImageToBeRead->d()) + (x * ImageToBeRead->d());
         r = *(buf+index+0);
         g = *(buf+index+1);
         b = *(buf+index+2);
        printf("%2x %2x %2x %d %d\\n",                                  // hex dump r/g/b
        (unsigned char)r,
		(unsigned char)g,
		(unsigned char)b,
         x,y);
         return;} {}
} 

Function {load_file(char *newfile)} {open return_type void
} {
  code {// this should be modified ... it is from Editor

Fl_New_File *Prev =(Fl_New_File*)txt_Tab_Edit->value();  // get active tab
if(Prev)
{
  while (Prev->pNext)
    Prev=Prev->pNext; // get the last one 
}

   txt_Tab_Edit->begin();
   char justFileName[30];
   char*scancp=newfile;
   int scanp= strlen(newfile)-1;
   char OLDjustFileName[30];
   snprintf(OLDjustFileName,1, "%c", '\\0');
   snprintf(justFileName,1, "%c", '\\0');
    char tempChar;
   while (((int)(scancp[scanp])!=0x2f)&&((int)(scancp[scanp])!=0x5c))  // 0x2f= /
      {
       
       tempChar=scancp[scanp];
       strcpy(OLDjustFileName,justFileName);
       if (!(strlen(OLDjustFileName)==0))
       {

          snprintf(justFileName,strlen(OLDjustFileName)+2, "%c%s", tempChar,OLDjustFileName);
       }
       else
       {
         snprintf(justFileName,2, "%c", tempChar);
       }
       scanp=scanp-1;
      }
   char buf[30];
   snprintf(buf,30,"%s",justFileName);
   Fl_New_File *st=new Fl_New_File(230,125,1050,840,"");
   st->copy_label(buf);
   //snprintf(st->fileName, strlen(newfile),"%s",newfile); // save file name 
    strcpy(st->fileName, newfile); // save file name 
    

  int r;
     r = st->t->loadfile(newfile);
  if (r)
   {
	if (strlen(newfile)==0)
          return;
        else
         fl_alert("Error reading from file \\'%s\\':\\n%s.", newfile, strerror(errno));
    }

  else

st->pPrev=Prev; // make the link
if(Prev)
  Prev->pNext=st; // make the link
st->NotSaved=0; // Not saved
Fl_New_File::counter++;
  st->t->call_modify_callbacks();
  txt_Tab_Edit->end();
  txt_Tab_Edit->value(st);
  txt_Tab_Edit->redraw();} {}
} 

